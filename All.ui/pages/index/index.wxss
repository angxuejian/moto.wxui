/**index.wxss**/

.item-ui {
  width: 90%;
  background-color: #fff;
  border-radius: 10px;
  margin: 10px auto;
  padding: 10px 2.5%;
}
.item-ui-title {
  font-size: 16px;
  color: #555;
  font-weight: 400;
  margin-bottom: 20px;
}

.all-component {
  display: flex;
  align-items: center;
  flex-wrap: wrap;

}
.item-component {
  width: 16%;
  margin: 0 2%;
  margin-bottom: 10px;
  display: flex;
  flex-direction: column;
  align-items: center;
  border-radius: 3px;
  padding: 3px 0;
}
.item-component .image {
  width: 30px;
  height: 30px;
  margin-bottom: 5px;
  background-size: cover;
}
.item-component text {
  color: #404040;
  font-size: 11px;
  width: 100%;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  text-align: center;
}


