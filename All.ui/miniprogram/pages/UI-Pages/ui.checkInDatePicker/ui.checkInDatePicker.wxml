<!--pages/UI-Pages/ui.checkInDatePicker/ui.checkInDatePicker.wxml-->
<view>

  <view class="item-btn">
    <text>Change事件</text>
    <view bindtap="open" data-multiple='{{true}}' hover-class="hover-font">打开</view>
  </view>

  <view class="item-btn">
    <text>单选</text>
    <view bindtap="open" data-multiple='{{false}}' hover-class="hover-font">打开</view>
  </view>
</view>

<checkin-date-picker multiple="{{multiple}}" bindchange='onCallbackChange' id='checkInDatePicker'></checkin-date-picker>