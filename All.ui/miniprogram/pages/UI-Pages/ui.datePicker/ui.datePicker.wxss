/* miniprogram/pages/UI-Pages/ui.datePicker/ui.datePicker.wxss */
page {
  padding-top: 15px;
}
.item-btn {
  height: 50px;
  width: 90%;
  padding: 0 5%;
  margin-bottom: 15px;
  display: flex;
  align-items: center;
  background-color: #fff;
}
.item-btn > text {
  font-size: 16px;
  color: #555;
  font-weight: 400;
  width: 30%;
  text-align: start;
}
.item-btn > view {
  width: 70%;
}

/** 
 * 修改 datePicker组件 显示样式
 * 只支持 font-size 和 color
 * 不填为 微信默认样式
*/

.date-picker {
  font-size: 16px;
  color: #333;
}