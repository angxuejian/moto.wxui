/**index.wxss**/
.navbar {
  position: fixed;
  top: 0;
  left: 0;
  background: #fff;
  width: 100%;
}
.navbar-title {
  position: absolute;
  bottom: 0;
  left: 0%;
  width: 100%;
  height: 44px;
  background-color: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
}
.navbar-title__list {
  position: absolute;
  left: 20px;
  width: 40px;
  height: 40px;
  display: flex;
  align-items: center;
  justify-content: flex-start;
}
.navbar-title__list image {
  width: 20px;
  height: 20px;
}




/* ----- */
.item-ui {
  width: 90%;
  background-color: #fff;
  border-radius: 10px;
  margin: 10px auto;
  padding: 10px 2.5%;
}
.item-ui-title {
  font-size: 16px;
  color: #555;
  font-weight: 400;
  margin-bottom: 20px;
}

.all-component {
  display: grid;
  grid-template-columns: 1fr 1fr 1fr 1fr 1fr;
  gap: 15px;
}
.item-component {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: flex-start;
  border-radius: 3px;
  padding: 3px 0;
}
.item-component .image {
  width: 30px;
  height: 30px;
  /* background-color: red; */
  margin-bottom: 5px;
  background-size: cover;
}
.item-component text {
  color: #404040;
  font-size: 11px;
  width: 100%;
  /* white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis; */
  text-align: center;
}


